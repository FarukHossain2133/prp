
<%- include('./templates/header') %>
<button  class="btn btn-info print" onclick="window.history.back()">Goto Back</button>
<button  class="btn btn-info print" style="border: 1px solid #777" onclick="window.print()">Print This Page</button>

    <div class="container mt-2">
        <div class="row">
            <div class="col-md-8 offset-md-2">
              <h5 class="text-center">Customer Ticket</h5>
            </div>
            <div class="col-12 p-2" style="text-align: right;">
                <span class="pr-3"><%- device._id %></span>
                <span><%- device.date.toDateString()%></span>
            </div>
            
            <div class="col-md-6">
                <table class="table table-striped">
                    <h4>User Information</h4>
                    <thead class="thead-dark">
                      <tr>
                        <th scope="col">Serial</th>
                        <th scope="col">Name</th>
                        <th scope="col">Value</th>
                        
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <th scope="row">1</th>
                        <td>Name</td>
                        <td><%- device.name %></td>
                      </tr>
                      <tr>
                        <th scope="row">2</th>
                        <td>Surname</td>
                        <td><%- device.surname %></td>
                      </tr>
                      <tr>
                        <th scope="row">4</th>
                        <td>Gender</td>
                        <td><%- device.gender %></td>
                      </tr>
                      <tr>
                        <th scope="row">5</th>
                        <td>Company</td>
                        <td><%- device.company %></td>
                      </tr>
                      <tr>
                        <th scope="row">6</th>
                        <td>Email</td>
                        <td><%- device.email %></td>
                      </tr>
                      <tr>
                        <th scope="row">7</th>
                        <td>Phone</td>
                        <td><%- device.phone %></td>
                      </tr>
                      <tr>
                        <th scope="row">8</th>
                        <td>Company</td>
                        <td><%- device.altphone %></td>
                      </tr>
                      <tr>
                        <th scope="row">9</th>
                        <td>Address</td>
                        <td><%- device.address %></td>
                      </tr>
                      <tr>
                        <th scope="row">10</th>
                        <td>Fax</td>
                        <td><%- device.fax %></td>
                      </tr>
                      <tr>
                        <th scope="row">11</th>
                        <td>City</td>
                        <td><%- device.city %></td>
                      </tr>
                      <tr>
                        <th scope="row">12</th>
                        <td>State</td>
                        <td><%- device.state %></td>
                      </tr>
                      <tr>
                        <th scope="row">13</th>
                        <td>Postal</td>
                        <td><%- device.postal %></td>
                      </tr>
                    </tbody>
                  </table>
                </div>

                <div class="col-md-6">
                    
                    <table class="table table-striped">
                        <h4>Device Information</h4>
                        <thead class="thead-dark">
                          <tr>
                            <th scope="col">Serial</th>
                            <th scope="col">Name</th>
                            <th scope="col">Value</th> 
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <th scope="row">1</th>
                            <td>Device Name</td>
                            <td><%- device.device %></td>
                          </tr>
                          <tr>
                            <th scope="row">2</th>
                            <td>Model</td>
                            <td><%- device.model %></td>
                          </tr>
                          <tr>
                            <th scope="row">3</th>
                            <td>Color</td>
                            <td><%- device.color %></td>
                          </tr>
                          <tr>
                            <th scope="row">4</th>
                            <td>Network</td>
                            <td><%- device.network %></td>
                          </tr>
                          <tr>
                            <th scope="row">5</th>
                            <td>IMEI</td>
                            <td><%- device.imei %></td>
                          </tr>
                          <tr>
                            <th scope="row">6</th>
                            <td>S/N</td>
                            <td><%- device.serial %></td>
                          </tr>
                          <tr>
                            <th scope="row">7</th>
                            <td>Weight</td>
                            <td><%- device.weight %></td>
                          </tr>
                          <tr>
                            <th scope="row">8</th>
                            <td>Voltage</td>
                            <td><%- device.voltage %></td>
                          </tr>
                          <tr>
                            <th scope="row">9</th>
                            <td>Status</td>
                            <td><%- device.status %></td>
                          </tr>
                          <tr>
                            <th scope="row">10</th>
                            <td>Fault</td>
                            <td><%- device.fault %></td>
                          </tr>
                          <tr>
                            <th scope="row">11</th>
                            <td>Problem</td>
                            <td><%- device.problem %></td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                    <div class="col-md-8 offset-md-2">
                      <h5 class="text-center">We Provide you best service</h5>
                      <p class="text-center">Please show the ticket when you collect the device from us</p>
                    </div>
            </div>
        </div>
  



  <%- include('./templates/footer') %>
